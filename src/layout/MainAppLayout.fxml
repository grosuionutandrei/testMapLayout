<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<StackPane fx:id="mainLayout" minHeight="-Infinity" minWidth="-Infinity" prefHeight="768.0" prefWidth="1024.0"
           xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" StackPane.alignment="CENTER">
            <children>
                <HBox fx:id="logo" alignment="CENTER" maxHeight="-Infinity" prefHeight="60.0"
                      style="-fx-background-color: red;"/>
                <HBox alignment="CENTER" prefHeight="711.0" prefWidth="1024.0" style="-fx-background-color: blue;"
                      VBox.vgrow="ALWAYS">
                    <children>
                        <StackPane fx:id="menu" minWidth="40.0" prefHeight="0.0" prefWidth="60.0"
                                   style="-fx-background-color: purple;" HBox.hgrow="ALWAYS">
                            <children>
                                <VBox fx:id="mapContainer" alignment="CENTER" prefHeight="710.0" prefWidth="653.0"
                                      StackPane.alignment="CENTER_RIGHT">
                                    <StackPane.margin>
                                        <Insets left="100.0"/>
                                    </StackPane.margin>
                                </VBox>
                                <StackPane fx:id="controls" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0"
                                           style="-fx-background-color: green;" StackPane.alignment="CENTER_LEFT">
                                    <children>
                                        <VBox alignment="CENTER_RIGHT" spacing="10.0" StackPane.alignment="CENTER">
                                            <children>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                                                           height="26.0" stroke="BLACK" strokeType="INSIDE"
                                                           width="43.0"/>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                                                           height="26.0" stroke="BLACK" strokeType="INSIDE"
                                                           width="43.0"/>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                                                           height="26.0" stroke="BLACK" strokeType="INSIDE"
                                                           width="43.0"/>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                                                           height="26.0" stroke="BLACK" strokeType="INSIDE"
                                                           width="43.0"/>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                                                           height="26.0" stroke="BLACK" strokeType="INSIDE"
                                                           width="43.0"/>
                                                <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </VBox>
                                    </children>
                                </StackPane>
                                <VBox fx:id="icons" alignment="CENTER" maxWidth="-Infinity" prefHeight="710.0"
                                      prefWidth="100.0" spacing="10.0" style="-fx-background-color: pink;"
                                      StackPane.alignment="CENTER_LEFT">
                                    <children>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="26.0"
                                                   stroke="BLACK" strokeType="INSIDE" width="43.0"/>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="26.0"
                                                   stroke="BLACK" strokeType="INSIDE" width="43.0"/>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="26.0"
                                                   stroke="BLACK" strokeType="INSIDE" width="43.0"/>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="26.0"
                                                   stroke="BLACK" strokeType="INSIDE" width="43.0"/>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="26.0"
                                                   stroke="BLACK" strokeType="INSIDE" width="43.0"/>
                                        <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </padding>
                                </VBox>
                            </children>
                        </StackPane>
                    </children>
                </HBox>
            </children>
        </VBox>
        <StackPane fx:id="firstLayout" disable="true" prefHeight="768.0" prefWidth="1024.0" visible="false"/>
        <StackPane fx:id="secondLayout" disable="true" prefHeight="768.0" prefWidth="1024.0" visible="false"/>
    </children>
</StackPane>

